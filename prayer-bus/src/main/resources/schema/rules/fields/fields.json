{
    "debug":"__fields__",
    "required":{
        "value":[
            "name",
            "type",
            "columnName",
            "columnType"
        ],
        "error":{
            "name":"RequiredAttrMissingException"
        }
    },
    "optional":{
        "value":[
            "pattern",
            "validator",
            "length",
            "datetime",
            "dateformat",
            "precision",
            "unit",
            "maxLength",
            "minLength",
            "max",
            "min",
            "primarykey",
            "unique",
            "subtable",
            "foreignkey",
            "nullable",
            "refTable",
            "refId"
        ],
        "error":{
            "name":"UnsupportAttrException"
        }
    },
    "pattern":{
        "value":{
            "name":"[A-Za-z]{1}[A-Za-z0-9]+",
            "type":"(BooleanType|IntType|LongType|DecimalType|DateType|StringType|JsonType|XmlType|ScriptType|BinaryType)",
            "columnName":"[A-Z]{1,3}\\_[A-Z]{1}[A-Z\\_0-9]*",
            "columnType":"(BOOLEAN|INT|LONG|DECIMAL|DATE|STRING|JSON|XML|SCRIPT|BINARY)[0-9]*"
        },
        "error":{
            "name":"PatternNotMatchException"
        }
    },
    "duplicated":{
        "value":[
            "name",
            "columnName"
        ],
        "errors":{
            "name":{
                "name":"DuplicatedAttrException"
            },
            "columnName":{
                "name":"DuplicatedColumnException"
            }
        }
    },
    "db.mapping":{
        "value":{
            "check":true
        },
        "error":{
            "name":"DBMappingConflictException",
            "arguments":["columnType"]
        }
    },
    "db.vector":{
        "value":{
            "check":true
        },
        "error":{
            "name":"VectorWrongException",
            "arguments":["type","columnType"],
            "append":true
        }
    }
}